{
    version: "v14",
    data_dir: "./data4/",
    cache_dir: "./data/cache/",
    logs_dir: "./logs/",
    batch_size: 40,
    num_workers: 2,
    lr: 0.0001,
    scheduler_params: {
        mode: "min",
        factor: 0.5,
        patience: 5,
    },
    weight_decay: 0.0001,
    debug: true,
    epochs: 1000,
    gradient_accumulation_steps: 128,
    model_save_path: "./models/model_data",
    num_classes: {
        note_type: 2,
        instrument: 5,
        pitch: 128,
    },
    model_structure: {
        dimension: 1024,
        num_encoder_layers: 4,
        num_decoder_layers: 6,
        backbone_type: "CustomTokenizedBackbone",
        pretrain: true,
        positional_embedding: "2d",
        time_series_type: "default",
        number_of_layers: 16,
        regression_activation_last_layer: "relu",
        classification_activation_last_layer: "None",
        MLP_skip: 3,
    },
    max_objects: 150,
    cost_note_type: 1,
    cost_instrument: 1,
    cost_pitch: 1,
    cost_regression: 0.0001,
    start_time_scaler: 25,
    duration_scaler: 2,
    velocity_scaler: 200,
    transforms: {
        type: "CQT",
        params: {},
    },
    loss: {
        use_softmax: true,
    },
    device: "cuda",
}