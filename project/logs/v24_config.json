{
    version: "v24",
    data_dir: "./data4/",
    cache_dir: "./data/cache/",
    logs_dir: "./logs/",
    batch_size: 20,
    num_workers: 2,
    lr: 0.001,
    scheduler_params: {
        mode: "min",
        factor: 0.5,
        patience: 5,
    },
    weight_decay: 0.0001,
    debug: false,
    epochs: 200,
    gradient_accumulation_steps: 256,
    model_save_path: "./models/model_data",
    num_classes: {
        note_type: 2,
        instrument: 5,
        pitch: 128,
    },
    model_structure: {
        dimension: 1024,
        num_encoder_layers: 3,
        num_decoder_layers: 6,
        backbone_type: "resnet18",
        pretrain: false,
        positional_embedding: "2d",
        time_series_type: "default",
        number_of_layers: 18,
        regression_activation_last_layer: "relu",
        classification_activation_last_layer: "None",
        MLP_skip: 3,
    },
    max_objects: 150,
    cost_note_type: 1,
    cost_instrument: 1,
    cost_pitch: 1,
    cost_regression: 1,
    start_time_scaler: 1,
    duration_scaler: 1,
    velocity_scaler: 1,
    transforms: {
        type: "CQT",
        params: {
            hop_length: 128,
            n_bins: 192,
            bins_per_octave: 36,
            fmin: 27.5,
        },
    },
    loss: {
        use_softmax: true,
    },
    device: "cuda",
}