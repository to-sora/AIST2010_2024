{
    version: "v6",
    data_dir: "./data4/",
    cache_dir: "./data/cache/",
    logs_dir: "./logs/",
    batch_size: 2,
    num_workers: 2,
    lr: 0.0001,
    scheduler_type: "ExponentialLR",
    scheduler_params: {
        mode: "min",
        factor: 0.5,
        patience: 5,
    },
    weight_decay: 0.0001,
    debug: false,
    epochs: 100,
    gradient_accumulation_steps: 32,
    model_save_path: "./models/model_data",
    num_classes: {
        note_type: 2,
        instrument: 5,
        pitch: 128,
    },
    model_structure: {
        dimension: 512,
        num_encoder_layers: 6,
        num_decoder_layers: 4,
        backbone_type: "CustomTokenizedBackbone",
        pretrain: true,
        positional_embedding: "sinusoid",
        time_series_type: "default",
        number_of_layers: 3,
        regression_activation_last_layer: "relu",
    },
    max_objects: 100,
    cost_note_type: 1,
    cost_instrument: 1,
    cost_pitch: 5,
    cost_regression: 1,
    data_nftt: 4096,
    data_win_length: 4096,
    data_hop_length: 256,
    data_power: 2,
    device: "cuda",
}